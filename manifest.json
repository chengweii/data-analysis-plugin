{
	"manifest_version": 2,
	"name": "数据分析助手",
	"version": "1.0.0",
	"description": "数据分析助手",
	"icons": {
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_title": "数据分析助手"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self' 'unsafe-eval'",
	"permissions": [
		"contextMenus",
		"tabs",
		"notifications",
		"webRequest",
		"webRequestBlocking",
		"storage",
		"http://*/*",
		"https://*/*"
	]
}